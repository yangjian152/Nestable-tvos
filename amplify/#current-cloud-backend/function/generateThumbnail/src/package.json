{
  "name": "generateThumbnail",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "ffmpeg:download": "curl -o ffmpeg.tar.xz https://johnvansickle.com/ffmpeg/releases/ffmpeg-release-amd64-static.tar.xz",
    "ffmpeg:extract": "tar -xvf ffmpeg.tar.xz",
    "ffmpeg:clean": "rm -rf ffmpeg && rm ffmpeg.tar.xz && mv ffmpeg-* ffmpeg",
    "ffmpeg": "npm run ffmpeg:download && npm run ffmpeg:extract && npm run ffmpeg:clean",
    "postinstall": "npm run ffmpeg"
  },
  "devDependencies": {},
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "hasbin": "^1.2.3"
  }
}
